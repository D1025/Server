void map_init(Map&map,bool firstTime)
{
	map.SetEvent((7),"_OnLeaveMap");
	if(firstTime)
	SetQuestGarbager(3*60,0,map.GetLocation().Id,0,0); 
	
	switch(map.GetProtoId())
	{
		case(558):
		case(560):
		case(562):
		CreateTimeEvent(((__FullSecond)+(((15)*__TimeMultiplier*60))),"e_DeleteStartLocation",map.GetLocation().Id,false);break;
	}
	
}

void _OnLeaveMap(Map&map,Critter&cr)
{
	if((@cr.GetMap()!=null))
	return;
	
	Location@loc=map.GetLocation();
	if((@loc!=null))
	DeleteLocation(loc.Id);
	else
	Log("Invalid starter location.");
}

uint e_DeleteStartLocation(array<uint>@LocData)
{
	Location@loc=GetLocation(LocData[0]);
	if(!(@loc!=null))return 0;
	
	DeleteLocation(loc.Id);
	return 0;
}
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               

void item_init(Item&item,bool firstTime)
{
	if(!firstTime||!(((item.Flags)&((0x20000000)))!=0))
	return;

uint e_Spawn(array<uint>@values)
{
	Item@item=GetItem(values[0]);
	if(!(@item!=null))
	return 0;  
	
	return spawners[item.Val4].Spawn(item);
} 

bool IsSpecialItem(Item&item)
{
	uint16 pid=item.GetProtoId();
	
	return pid==(281)||pid==(282)
	||pid==(623)||pid==(82)||pid==(25444);
} 

bool InvalidIndex(uint num)
{
	return(30)<num&&num<spawners.length();
}  

IItemSpawner@GetSpawner(uint num)
{
	if(InvalidIndex(num))
	return null;
	return spawners[num];
}

IItemSpawner@MakeSpawner(uint num)
{
	if(num<=(30))
	return null;
	if(num>=spawners.length())
	spawners.resize(num+1);
	@spawners[num]=@ItemSpawner();
	return spawners[num];
}

void MakeSpawnerContainer(Item&item,uint type)
{
	if(!Init)
	ItemSpawnInit();
	item.Val4=type;
	spawners[item.Val4].Spawn(item);
	if(spawners[item.Val4].SpawnTimeMax>0)
	CreateTimeEvent(((__FullSecond)+(spawners[type].GetSpawnTime())),"e_Spawn",item.Id,false);
}


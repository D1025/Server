void dlg_ShrenRumour(Critter&player,Critter@npc,string@text)
{
	if(!((@text!=null)&&text.length()==0))
	return;
	
	float interests=0;
	uint bank=0;
	for(uint i=1;i<(7);i++)
	{
		float newinterests=BankGetSaveInterest(i);
		if(interests<newinterests)
		{
			interests=newinterests;
			bank=i;
		}
	}
	text="$bank";
	if(interests==0)
	bank=Random(0,(7)-1);
	switch(bank)
	{
		case(0):
		text+="Vault City";
		break;
		case(1):
		text+="Redding";
		break;
		case(2):
		text+="Broken Hills";
		break;
		case(3):
		text+="San Francisco";
		break;
		case(4):
		text+="NCR";
		break;
		case(5):
		text+="Hub";
		break;
		case(6):
		text+="Salt Lake City";
		break;
		default:
	}
	
}    

